{
  "name": "xrefparser",
  "version": "2.2.0",
  "description": "OpenEdge XREF parser",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register lib/**/*.spec.ts",
    "compile": "tsc && npm run test",
    "todist": "deploy.sh",
    "publish_patch": "npm run compile && npm version patch && npm run todist && npm publish dist",
    "publish_minor": "npm run compile && npm version minor && npm run todist && npm publish dist",
    "publish_major": "npm run compile && npm version major && npm run todist && npm publish dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfv/xrefparser.git"
  },
  "keywords": [
    "openedge",
    "xref",
    "db"
  ],
  "author": "bfv <dev@bfv.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bfv/xrefparser/issues"
  },
  "homepage": "https://github.com/bfv/xrefparser#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.4",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "@types/node": "^10.3.2"
  }
}
